# Yarn
alias ya='yarn'

# Yarn run
alias yar='yarn run'
alias yars='yarn run start'
alias yart='yarn run test'
alias yarb='yarn run build'

# Yarn add/remove
alias yaa='yarn add'
alias yarm='yarn remove'

# Yarn global
alias yaga='yarn global add'
alias yagrm='yarn global remove'
alias yagul='yarn global upgrade --latest'